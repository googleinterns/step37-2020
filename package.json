{
  "name": "recommendations-impact-dashboard",
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine src/**/*.spec.ts",
    "build": "ng build && cp -r src/main/angular/WEB-INF src/main/webapp/WEB-INF && mvn clean && mvn package appengine:run -DskipTests",
    "deploy": "ng build && cp -r src/main/angular/WEB-INF src/main/webapp/WEB-INF && mvn clean && mvn package appengine:deploy -DskipTests && mvn package appengine:deployCron",
    "serve": "ng serve --open",
    "check": "gts check src/**/*.ts",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix **/*.ts",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "dependencies": {
    "@angular/animations": "^10.0.3",
    "@angular/cdk": "^10.0.1",
    "@angular/common": "^10.0.3",
    "@angular/compiler": "^10.0.3",
    "@angular/core": "^10.0.3",
    "@angular/forms": "^10.0.3",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "^10.0.3",
    "@angular/platform-browser-dynamic": "^10.0.3",
    "@angular/router": "^10.0.3",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "angular-google-charts": "^1.1.4",
    "jasmine": "^3.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jasminewd2": "^2.2.0",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "rxjs": "^6.5.5",
    "ts-mockito": "^2.6.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.2",
    "@angular/cli": "^10.0.2",
    "@angular/compiler-cli": "^10.0.3",
    "@types/node": "^13.13.14",
    "codelyzer": "^6.0.0",
    "gts": "^2.0.2",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "^3.9.6"
  }
}
