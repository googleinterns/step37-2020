{
  "name": "recommendations-impact-dashboard",
  "scripts": {
    "test": "mocha -r ts-node/register src/test/**/*.spec.ts",
    "build": "ng build && cp -r src/main/angular/WEB-INF src/main/webapp/WEB-INF && mvn package appengine:run",
    "serve": "ng serve --open",
    "check": "gts check src/**/*.ts",
